<Window x:Class="MathZone.Biography"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Biography" Height="700" Width="600" WindowState="Maximized">
    <DockPanel>
        <Button x:Name="backBtn" Content="Back" DockPanel.Dock="Top" HorizontalAlignment="Right" Width="60" Click="backBtn_Click"/>
        <ComboBox x:Name="nameBox" DockPanel.Dock="Top" Width="200" VerticalAlignment="Top">
            <ComboBoxItem IsSelected="True">Euclid</ComboBoxItem>
            <ComboBoxItem>Carl Fridrich Guass</ComboBoxItem>
        </ComboBox>
        <WebBrowser x:Name="browser"/>
    </DockPanel>
</Window>
